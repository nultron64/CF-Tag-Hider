{
    "manifest_version": 3,
    "name": "CF Tag Hider",
    "version": "1.0",
    "description": "Inject a button to hide/unhide tags on a code forces website",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": [
      "https://codeforces.com/*/problem/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://codeforces.com/*/problem/*"],
        "js": ["contentScript.js"]
      }
    ]
}
  